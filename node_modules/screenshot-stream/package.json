    "get-port": "^3.1.0",
    "get-stream": "^3.0.0",
    "image-size": "^0.5.4",
    "is-jpg": "^1.0.0",
    "is-png": "^1.0.0",
    "pify": "^3.0.0",
    "png-js": "^0.1.1",
    "xo": "*"
  },
  "xo": {
    "esnext": true,
    "overrides": [
      {
