{
  "name": "screenshot-stream",
  "version": "4.2.0",
  "description": "Capture screenshot of a website and return it as a stream",
  "license": "MIT",
  "repository": "kevva/screenshot-stream",
  "author": {
    "name": "Kevin MÃ¥rtensson",
